def bubble_sort(arr):
    n=len(arr)
    for i in range(n):
        for j in range(0,n-i-1):
            if arr[j]>arr[j+1]:
                arr[j],arr[j+1]=arr[j+1],arr[j]
    return arr
unsorted=[43,22,6,2,56,57,88,1]
print("unsorted : ",unsorted)
sorted=bubble_sort(unsorted)
print("sorted : ",sorted)
